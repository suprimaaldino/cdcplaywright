{
  "name": "cdcplaywright",
  "version": "1.0.0",
  "description": "Repository for CDC self-learning with Playwright automation",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:all-browsers": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:headed": "playwright test --headed",
    "test:chrome:headed": "playwright test --project=chromium --headed",
    "test:firefox:headed": "playwright test --project=firefox --headed",
    "test:safari:headed": "playwright test --project=webkit --headed",
    "test:all-browsers:headed": "playwright test --project=chromium --project=firefox --project=webkit --headed",
    "test:api": "playwright test tests/api/",
    "test:visual": "playwright test tests/visual/",
    "test:smoke": "playwright test --grep=@smoke",
    "test:regression": "playwright test --grep=@regression",
    "test:non-flaky": "playwright test --grep-invert=@flaky",
    "test:smoke-headed": "playwright test --grep=@smoke --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright test --reporter=html",
    "test:update-snapshots": "playwright test --update-snapshots",
    "test:list": "playwright test --list",
    "test:trace": "playwright test --trace=on",
    "test:ui": "playwright test --ui",
    "prepare": "npx playwright install --with-deps",
    "lint": "eslint . --ext .ts",
    "show-report": "npx playwright show-report",
    "codegen": "npx playwright codegen"
  },
  "keywords": [
    "automation",
    "testing",
    "CDC"
  ],
  "author": "YourName",
  "license": "ISC",
  "devDependencies": {
    "@applitools/eyes-playwright": "^1.37.3",
    "@playwright/test": "^1.53.2",
    "@types/node": "^24.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^0.18.0",
    "typescript": "^5.4.5"
  }
}
